        <bm:field name="templet_header_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="TEMPLET_HEADER_ID" prompt="PRJ_CDD_ITEM_TPLT_TAB_GROUP.TEMPLET_HEADER_ID"/>
