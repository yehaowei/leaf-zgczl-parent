<bm:model xmlns:e="aurora.service.exception" xmlns:bm="http://www.leaf-framework.org/schema/bm" xmlns:f="aurora.database.features" alias="t1" baseTable="PRJ_CDD_ITEM_TPLT_TAB_GROUP" defaultOrderBy="t1.seq_num asc">
    <bm:fields>
        <bm:field name="cdd_item" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="CDD_ITEM" prompt="PRJ_CDD_ITEM_TPLT_TAB_GROUP.CDD_ITEM"/>
