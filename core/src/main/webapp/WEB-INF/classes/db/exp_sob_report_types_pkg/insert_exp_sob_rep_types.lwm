<bm:model xmlns:bm="http://www.leaf-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="insert">
            <bm:update-sql><![CDATA[
            begin
             EXP_SOB_REPORT_TYPES_PKG.insert_exp_sob_rep_types(
                                                               ${/session/@company_id},
                                                               ${@set_of_books_id},
                                                               ${@expense_report_type_code},
                                                               ${@description},
                                                               ${@document_page_type},
                                                               ${@currency_code},
                                                               ${@coding_rule},
                                                               ${@auto_approve_flag},
                                                               ${@auto_audit_flag},
                                                               ${@payment_object},
                                                               ${@req_required_flag},
                                                               ${@adjustment_flag},
                                                               ${@report_name},
                                                               ${@payment_flag},
                                                               ${@amortization_flag},
                                                               ${@enabled_flag},
                                                               ${/session/@user_id},
                                                               ${@template_flag},
                                                               ${@reserve_budget},
                                                               ${@budget_control_enabled},
                                                               ${@expense_report_type_id});
            end;
     ]]></bm:update-sql>
        </bm:operation>
        <bm:operation name="update">
            <bm:update-sql><![CDATA[ 
                begin
                  exp_sob_report_types_pkg.update_exp_sob_rep_types(${@expense_report_type_id},
                                                                   ${@description_id},
                                                                   ${@description},
                                                                   ${@document_page_type},
                                                                   ${@coding_rule},
                                                                   ${@auto_approve_flag},
                                                                   ${@auto_audit_flag},
                                                                   ${@payment_object},
                                                                   ${@req_required_flag},
                                                                   ${@adjustment_flag},
                                                                   ${@currency_code},
                                                                   ${@report_name},
                                                                   ${@payment_flag},
                                                                   ${@amortization_flag},
                                                                   ${@enabled_flag},
                                                                   ${/session/@user_id},
                                                                   ${@template_flag},
                                                                   ${@reserve_budget},
                                                                   ${@budget_control_enabled});
			     end;
     ]]></bm:update-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
