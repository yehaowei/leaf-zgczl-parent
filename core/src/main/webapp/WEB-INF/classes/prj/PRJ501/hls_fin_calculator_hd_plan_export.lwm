        <bm:field name="lease_item_name" expression="(prj305_save_pkg.get_prj_quotation_lease_item(${@quotation_id}))"/>
        <!-- <bm:field name="lease_item_name" expression="(select i.full_name   from prj_project_lease_item i,prj_quotation q   where i.contract_seq=q.contract_seq   and i.project_id=q.document_id   and q.document_category=&apos;PROJECT&apos;    and q.quotation_id=${@quotation_id})"/> -->
        <bm:field name="net_asset_value" expression="(select to_char(sum(i.net_asset_value), &apos;FM99,999,999,990.00&apos;)   from prj_project_lease_item i,prj_quotation q   where i.contract_seq=q.contract_seq   and i.project_id=q.document_id   and q.document_category=&apos;PROJECT&apos;    and q.quotation_id=${@quotation_id})"/>
        <bm:field name="original_asset_value" expression="(select to_char(sum(i.original_asset_value), &apos;FM99,999,999,990.00&apos;)   from prj_project_lease_item i,prj_quotation q   where i.contract_seq=q.contract_seq   and i.project_id=q.document_id   and q.document_category=&apos;PROJECT&apos;    and q.quotation_id=${@quotation_id})"/>
        <bm:field name="net_finance_amount_n" expression="to_char(t1.finance_amount-t1.deposit,&apos;FM99,999,999,990.00&apos;)"/>
        <bm:field name="int_rate_fixing_way_n" expression="(select v.code_value_name from sys_code_values_v v where v.code=&apos;TRE502_INT_RATE_FIXING_WAY&apos; and v.code_value=t1.int_rate_fixing_way)"/>
        <bm:field name="quotation_business_type" expression="(select q.business_type from prj_quotation q where q.quotation_id=${@quotation_id})"/>
        <bm:field name="lease_charge_fm" expression="decode(t1.lease_charge,&apos;0&apos;,&apos;0&apos;,to_char(t1.lease_charge,&apos;fm999,999,999,990.00&apos;))"/>
        <bm:field name="residual_value_fm" expression="decode(t1.residual_value,&apos;0&apos;,&apos;0&apos;,to_char(t1.residual_value,&apos;fm999,999,999,990.00&apos;))"/>
        <bm:field name="tax_total_fm" expression="decode(t1.hd_user_col_n02,&apos;0&apos;,&apos;0&apos;, to_char(t1.hd_user_col_n02,&apos;fm999,999,999,990.00&apos;))"/>
        <bm:field name="roe_persent" expression="decode(t1.hd_user_col_n07,&apos;&apos;,&apos;&apos;,(t1.hd_user_col_n07*100)||&apos;%&apos;)"/>
        <bm:field name="rental_total" expression="(select decode(SUM(nvl(a.rental,0)),0,&apos;0&apos;,to_char(SUM(nvl(a.rental,0)),&apos;FM999,999,999,990.00&apos;)) from hls_fin_calculator_ln a where a.calc_session_id=t1.calc_session_id and a.times&gt;=1)"/>
        <bm:field name="vat_interest_total" expression="(select decode(SUM(nvl(a.vat_interest,0)),0,&apos;0&apos;,to_char(SUM(nvl(a.vat_interest,0)),&apos;FM999,999,999,990.00&apos;)) from hls_fin_calculator_ln a where a.calc_session_id=t1.calc_session_id and a.times&gt;=1)"/>
    </bm:fields>
    <bm:features>
        <f:standard-who/>
