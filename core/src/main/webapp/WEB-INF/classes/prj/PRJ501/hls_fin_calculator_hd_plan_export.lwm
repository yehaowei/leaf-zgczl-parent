    <bm:primary-key>
        <bm:pk-field name="calc_session_id"/>
    </bm:primary-key>
    <bm:data-filters>
        <bm:data-filter enforceOperations="query" expression="t1.calc_session_id=(select q.calc_session_id from prj_quotation q where q.quotation_id=${@quotation_id})"/>
    </bm:data-filters>
</bm:model>
