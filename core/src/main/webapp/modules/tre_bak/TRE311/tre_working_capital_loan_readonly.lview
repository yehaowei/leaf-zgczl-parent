                    <a:field name="currency_desc" readOnly="true"/>
                    <a:field name="annual_days" readOnly="true"/>
                    <a:field name="loan_amount" readOnly="true"/>
                    <a:field name="loan_amount_type_desc" displayField="code_value_name" options="working_capital_loan_type_ds" readOnly="true" required="true" returnField="loan_amount_type" valueField="code_value"/>
                    <!-- <a:field name="interest_rate_decisions_desc" displayField="code_value_name" options="interest_rate_decisions_ds" required="true" returnField="int_rate_fixing_day" valueField="code_value"/>
                    <a:field name="benchmark_interest_rate_term_desc" displayField="code_value_name" options="benchmark_interest_rate_term_ds" required="true" returnField="base_rate_method" valueField="code_value"/> -->
                    <a:field name="int_rate_fixing_way_desc" displayField="code_value_name" options="int_rate_fixing_way_ds" readOnly="true" returnField="int_rate_fixing_way" valueField="code_value"/>
                    <a:field name="calc_method_desc" displayField="code_value_name" options="interest_calculated_method_ds" readOnly="true" required="true" returnField="calc_method" valueField="code_value"/>
                    <a:field name="interest_period_desc" displayField="code_value_name" options="interest_settlement_frequency_ds" readOnly="true" required="true" returnField="interest_period" valueField="code_value"/>
                    <a:field name="interest_calc_date_desc" displayField="code_value_name" options="interest_expiry_date_ds" readOnly="true" returnField="interest_calc_date" valueField="code_value"/>
                    <a:field name="interest_calc_date" readOnly="true"/>
                    <a:field name="interest_payment_date_desc" displayField="code_value_name" options="interest_payment_date_ds" readOnly="true" returnField="interest_payment_date" valueField="code_value"/>
                    <a:field name="interest_payment_date" readOnly="true"/>
                    <a:field name="rate_float_cycle_desc" displayField="code_value_name" options="rate_float_cycle_ds" readOnly="true" required="true" returnField="rate_float_cycle" valueField="code_value"/>
                    <a:field name="flt_execute_times_day_desc" displayField="code_value_name" options="flt_execute_times_day_ds" readOnly="true" returnField="flt_execute_times_day" valueField="code_value"/>
                    <a:field name="withdrawal_method_desc" displayField="code_value_name" options="withdrawal_method_ds" readOnly="true" required="true" returnField="withdrawal_method" valueField="code_value"/>
                    <a:field name="repayment_method_desc" displayField="code_value_name" options="repayment_method_ds" readOnly="true" required="true" returnField="repayment_method" valueField="code_value"/>
                    <a:field name="adjust_day_desc" displayField="code_value_name" options="adjust_day_ds" readOnly="true" returnField="adjust_day" valueField="code_value"/>
                    <a:field name="interest_rate" readOnly="true"/>
                    <a:field name="loan_date_from" readOnly="true" validator="dateValidator"/>
                    <a:field name="loan_date_to" readOnly="true" validator="dateValidator"/>
                    <a:field name="consulting_fee" readOnly="true"/>
                    <a:field name="loan_charge" readOnly="true"/>
                    <a:field name="other_fee" readOnly="true"/>
                </a:fields>
                <a:events>
                    <a:event name="update" handler="loan_contract_ds_update"/>
                    <a:event name="load" handler="loanConLoadHandler"/>
                </a:events>
            </a:dataSet>
            <a:dataSet id="loan_contract_int_rate_ds" bindName="int_rate" bindTarget="loan_contract_ds" model="tre.tre503.tre_loan_contract_int_rate" selectable="true">
                <a:fields>
                    <a:field name="start_date_from" required="true"/>
                    <a:field name="int_rate" required="true"/>
                    <a:field name="loan_contract_id" defaultValue="${/parameter/@loan_contract_id}"/>
                </a:fields>
            </a:dataSet>
            <a:dataSet id="loan_con_withdrawal_plan_ds" bindName="withrawal_plan" bindTarget="loan_contract_ds" model="tre.tre503.tre_loan_con_withdrawal_plan" selectable="true">
                <a:fields>
                    <a:field name="widthdrawal_date" required="true"/>
                    <a:field name="amount" required="true"/>
                    <a:field name="loan_term" required="true"/>
                    <a:field name="loan_contract_id" defaultValue="${/parameter/@loan_contract_id}"/>
                </a:fields>
                <a:events>
                    <a:event name="update" handler="withdrawalPlanUpdate"/>
                </a:events>
            </a:dataSet>
            <a:dataSet id="loan_con_repayment_plan_ds" bindName="repayment_plan" bindTarget="loan_contract_ds" model="tre.tre503.tre_loan_con_repayment_plan" pageSize="10000" selectable="true">
                <a:fields>
                    <a:field name="repayment_type_desc" displayField="code_value_name" options="repayment_type_ds" required="true" returnField="repayment_type" valueField="code_value"/>
                    <a:field name="repayment_date" required="true"/>
                    <a:field name="amount" required="true"/>
                    <a:field name="repayment_type"/>
                    <a:field name="loan_contract_id" defaultValue="${/parameter/@loan_contract_id}"/>
                </a:fields>
                <a:events>
                    <a:event name="load" handler="repaymentPlanAdd"/>
                    <a:event name="add" handler="repaymentPlanAdd"/>
                    <a:event name="update" handler="repaymentPlanUpdate"/>
                    <!-- <a:event name="afterremove" handler="calRepaymentPlanHandle"/> -->
                </a:events>
            </a:dataSet>
            <a:dataSet id="cal_repayment_plan_ds" autoCreate="true">
                <a:fields>
                    <a:field name="repayment_frequency_desc" displayField="code_value_name" options="repayment_frequency_ds" returnField="repayment_frequency" valueField="code_value"/>
                    <a:field name="loan_contract_id" defaultValue="${/parameter/@loan_contract_id}"/>
                </a:fields>
            </a:dataSet>
            <a:dataSet id="loan_con_fin_con_ds" bindName="fin_con" bindTarget="loan_contract_ds" model="tre.tre503.tre_loan_con_fin_con" pageSize="10000" selectable="true">
                <a:fields>
                    <a:field name="contract_number" lovGridHeight="250" lovHeight="400" lovService="cont.CON500.con_contract_lov" lovWidth="500" required="true">
                        <a:mapping>
                            <a:map from="contract_id" to="contract_id"/>
                            <a:map from="contract_number" to="contract_number"/>
                            <a:map from="contract_name" to="contract_name"/>
                        </a:mapping>
                    </a:field>
                    <a:field name="loan_contract_id" defaultValue="${/parameter/@loan_contract_id}"/>
                </a:fields>
            </a:dataSet>
        </a:dataSets>
        <a:screenBody>
            <a:screenTopToolbar>
                <a:gridButton click="wcl_goBack" text="HLS.BACK"/>
                <a:gridButton id="wcl_review_id" click="wcl_review" text="TRE502.BUTTON.REVIEW"/>
