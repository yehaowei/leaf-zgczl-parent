                        Ext.fly('WC_planRepayment').setStyle({
                            display: 'none'
                        });
                        //清除数据
                        var ds = $('loan_con_repayment_plan_ds');
                        ds.removeAll();
                        //清除数据必输
                        //ds.getField('repayment_date').setRequired(false);
                        //ds.getField('amount').setRequired(false);
                    } else {
                        Ext.fly('WC_lastRepayment').setStyle({
                            display: 'none'
