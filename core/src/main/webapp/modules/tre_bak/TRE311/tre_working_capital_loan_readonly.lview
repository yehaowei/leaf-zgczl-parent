                        //设置提款字段不能为空
                        //var ds = $('loan_con_withdrawal_plan_ds');
                        //ds.getField('widthdrawal_date').setRequired(true);
                        //ds.getField('amount').setRequired(true);
                        //ds.getField('loan_term').setRequired(true);
                    }
                } else if (name == 'repayment_method') {
                    if (value == 'LAST_REPAYMENT_DATE') {
                        Ext.fly('WC_lastRepayment').setStyle({
                            display: ''
