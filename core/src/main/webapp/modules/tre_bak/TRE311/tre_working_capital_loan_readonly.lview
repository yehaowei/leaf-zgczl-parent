                                <a:comboBox name="rate_float_cycle_desc" bindTarget="loan_contract_ds" prompt="TRE_LOAN_CONTRACT.RATE_FLOAT_CYCLE"/>
                                <a:comboBox name="flt_execute_times_day_desc" id="cb_flt_execute_times_day" bindTarget="loan_contract_ds" prompt="TRE_LOAN_CONTRACT.FLT_EXECUTE_TIMES_DAY"/>
                                <a:comboBox name="adjust_day_desc" id="cb_adjust_day" bindTarget="loan_contract_ds" prompt="TRE_LOAN_CONTRACT.ADJUST_DAY"/>
                            </a:box>
                            <div id="WC_IntTable" style="display:none;">
                                <a:box style="margin-left:150px;">
                                    <a:fieldSet column="1" title="TRE502.FIELDSET.INTEREST_RATES_TABLE">
                                        <a:grid bindTarget="loan_contract_int_rate_ds" height="160" navBar="true" width="320">
                                            <a:columns>
                                                <a:column name="start_date_from" prompt="TRE_LOAN_CONTRACT_INT_RATE.START_DATE_FROM" renderer="Leaf.formatDate"/>
                                                <a:column name="start_date_to" prompt="TRE_LOAN_CONTRACT_INT_RATE.START_DATE_TO" renderer="Leaf.formatDate"/>
                                                <a:column name="int_rate" align="right" prompt="TRE_LOAN_CONTRACT_INT_RATE.INT_RATE"/>
                                            </a:columns>
                                        </a:grid>
                                    </a:fieldSet>
                                </a:box>
                            </div>
                        </a:fieldSet>
                        <!-- 提款方式 -->
                        <a:fieldSet column="1" title="TRE502.FIELDSET.WITHDRAW_METHOD">
                            <a:box column="1" labelWidth="120" width="280">
                                <a:comboBox name="withdrawal_method_desc" bindTarget="loan_contract_ds" prompt="TRE_LOAN_CONTRACT.WITHDRAWAL_METHOD"/>
                            </a:box>
                            <div id="WC_finalWithdraw" style="display:none;">
                                <a:box column="1" labelWidth="120" width="280">
                                    <a:datePicker name="last_withdrawal_date" bindTarget="loan_contract_ds" prompt="TRE_LOAN_CONTRACT.LAST_WITHDRAWAL_DATE" readOnly="true"/>
                                </a:box>
                            </div>
                            <div id="WC_planWithdraw" style="display:none;">
                                <a:box>
                                    <a:box style="margin-left:116px;">
                                        <a:fieldSet title="TRE502.FIELDSET.WITHDRAWAL_SCHEDULE">
                                            <a:grid bindTarget="loan_con_withdrawal_plan_ds" height="160" navBar="true" width="350">
                                                <a:columns>
                                                    <a:column name="widthdrawal_date" prompt="TRE_LOAN_CON_WITHDRAWAL_PLAN.WIDTHDRAWAL_DATE" renderer="Leaf.formatDate"/>
                                                    <a:column name="amount" align="right" prompt="TRE_LOAN_CON_WITHDRAWAL_PLAN.AMOUNT" renderer="Leaf.formatMoney"/>
                                                    <a:column name="loan_term" prompt="TRE_LOAN_CON_WITHDRAWAL_PLAN.LOAN_TERM" renderer="Leaf.formatDate"/>
                                                </a:columns>
                                            </a:grid>
                                        </a:fieldSet>
                                    </a:box>
                                </a:box>
                            </div>
                        </a:fieldSet>
                        <!-- 本金还款方式 -->
                        <a:fieldSet title="TRE502.FIELDSET.PRINCIPAL_REPAYMENT">
                            <a:box column="1" labelWidth="120" width="280">
                                <a:comboBox name="repayment_method_desc" bindTarget="loan_contract_ds" prompt="TRE_LOAN_CONTRACT.REPAYMENT_METHOD"/>
                            </a:box>
                            <div id="WC_lastRepayment" style="display:none;">
                                <a:box column="1" labelWidth="120" width="280">
                                    <a:datePicker name="last_repayment_date" bindTarget="loan_contract_ds" prompt="TRE_LOAN_CONTRACT.LAST_REPAYMENT_DATE" readOnly="true"/>
                                </a:box>
                            </div>
                            <div id="WC_planRepayment" style="display:none;">
                                <a:box>
                                    <a:box style="margin-left:115px;">
                                        <a:fieldSet title="TRE502.FIELDSET.REPAYMENT_SCHEDULE">
                                            <a:grid bindTarget="loan_con_repayment_plan_ds" height="200" navBar="true" width="420">
                                                <a:columns>
                                                    <a:column name="times" prompt="HLS.TIMES" width="60"/>
                                                    <a:column name="repayment_type_desc" prompt="TRE503.REPAYMENT_TYPE" renderer="type_renderer"/>
                                                    <a:column name="repayment_date" prompt="TRE_LOAN_CON_REPAYMENT_PLAN.REPAYMENT_DATE" renderer="Leaf.formatDate"/>
                                                    <a:column name="amount" align="right" prompt="TRE_LOAN_CON_REPAYMENT_PLAN.AMOUNT" renderer="Leaf.formatMoney"/>
                                                </a:columns>
                                            </a:grid>
                                        </a:fieldSet>
                                    </a:box>
                                </a:box>
                            </div>
                        </a:fieldSet>
                    </a:tab>
                    <a:tab prompt="合同审签单信息" width="100">
                        <a:form marginWidth="40" title="审签信息">
                            <a:box column="1" width="1120">
                                <a:hBox>
                                    <a:datePicker name="signing_date" bindTarget="loan_contract_ds" prompt="时间" readOnly="true" width="150"/>
                                    <a:textField name="loan_contreat_num" bindTarget="loan_contract_ds" prompt="编码" readOnly="true" width="150"/>
                                </a:hBox>
                                <a:hBox>
                                    <a:lov name="transact_unit_name" bindTarget="loan_contract_ds" prompt="经办部门" readOnly="true" width="150"/>
                                    <a:lov name="transactor" bindTarget="loan_contract_ds" prompt="经办人" readOnly="true" width="150"/>
                                </a:hBox>
                                <a:hBox>
                                    <a:textField name="loan_contract_name" bindTarget="loan_contract_ds" prompt="合同名称" readOnly="true" width="150"/>
                                </a:hBox>
                                <a:hBox>
                                    <a:textArea name="note" bindTarget="loan_contract_ds" prompt="合同摘要" readOnly="true" width="580"/>
                                </a:hBox>
                            </a:box>
                        </a:form>
                    </a:tab>
                    <a:tab prompt="附件资料" width="100">
                        <a:form marginWidth="40" title="附件信息">
                            <a:screen-include screen="atm_query.lview?document_id=${/parameter/@loan_contract_id}&amp;document_table=TRE_LOAN_CONTRACT&amp;showflag=N"/>
                        </a:form>
                    </a:tab>
                </a:tabs>
            </a:tabPanel>
